<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.olemiss.p2ststasny" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".bibleTriviaActivity" >
            <intent-filter>
                <action android:name="edu.olemiss.bibleTriviaActivity" />
                    <category
                        android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".calenderConverterActivity" >
            <intent-filter>
                <action android:name="edu.olemiss.calenderConverterActivity" />
                <category
                    android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
